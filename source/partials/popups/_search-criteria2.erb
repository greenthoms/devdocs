    <script>
    $(document).ready(function() {
        var window = $("#window2"),
            undo = $("#undo")
                    .bind("click", function() {
                        window.data("kendoWindow").open();
                        undo.hide();
                    });

        var onClose = function() {
            undo.show();
        }

        if (!window.data("kendoWindow")) {
            window.kendoWindow({
                width: "700px",
                title: "Search",
                actions: [
                    "Close"
                ],
                close: onClose
            });
        }
    });
</script>