<input id="datepicker" value="10/10/2011" style="width: 100%" />
<script>
    $(document).ready(function() {
        $("#datepicker").kendoDatePicker();

        $("#monthpicker").kendoDatePicker({
            start: "year",
            depth: "year",
            format: "MMMM yyyy"
        });
    });
</script>